.PHONY: test
test:
	rm -rf tmp && \
	mkdir -p tmp && \
	mkdir -p nimsforestfolders-copy && \
	cp -r $(CURDIR)/nimsforestfolders/* nimsforestfolders-copy/ && \
	cd tmp && \
	cp $(CURDIR)/MAKEFILE.nimsforestorganization . && \
	cp -r $(CURDIR)/docs . && \
	cp -r $(CURDIR)/nimsforestfolders-copy ../nimsforestfolders && \
	make -f MAKEFILE.nimsforestorganization nimsforestorganization-addtomainmake && \
	make nimsforestorganization-hello && \
	DOCS_PATH=. make nimsforestorganization-init && \
	DOCS_PATH=. make nimsforestorganization-lint && \
	cd .. && rm -rf nimsforestfolders-copy